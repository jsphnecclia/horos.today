    <section class="hero" id="home">
      <div class="hero-inner">
        <div class="" id="lunar-card">
          <div id="sun-sign">
            <h5> SUN in <%= @sun %> </h5>

          </div>
          <div class="lunar-transit">
            <h1> LUNAR SIGN </h1>
            <h5 class="lunar-transit-inner" ><%= @lunar_transit %> </h5>
          </div>
          <div class="lunar-aspects">
            <h1> LUNAR ASPECTS </h1>
            <ul>
              <% @aspects.each do |key, val| %>
                <li><%= "MOON #{val[0]} #{HorosToday::PLANETS[key]} " \
                  "#{Time.at(val[1][3]*3600).utc.strftime("%H:%M")}"%>
                </li>
              <% end %>
            </ul>
          </div>
        </div>
      </div>
    </section>
    <main>
      <div class="exp-container" id="explanations">

        <ul id="nav-bar" class="nav nav-tabs nav-justified">
          <li class="nav-item"><a class="nav-link rounded-0" data-toggle="tab" href="#planets">PLANETS</a></li>
          <li class="nav-item"><a class="nav-link rounded-0" data-toggle="tab" href="#signs">SIGNS</a></li>
